{
	"manifest_version": 2,
	"name": "Koi Pixiv Extension",
	"version": "1.0.1",
	"description": "Make Pixiv Browsing Better",
	"icons": {"48": "icon48.PNG",
			 "128": "icon128.PNG"},
	"content_scripts": [{
		"js": ["jquery-2.1.3.min.js", "contentscript.js"],
		"matches": ["*://*.pixiv.net/*"]
		}],
	"permissions": [
		"*://*.pixiv.net/*"
	],	
	"web_accessible_resources": ["script.js"]
}