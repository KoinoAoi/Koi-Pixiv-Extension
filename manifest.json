{
	"manifest_version": 2,
	"name": "Koi Pixiv Extension",
	"version": "1.1.2",
	"description": "Make Pixiv Browsing Better",
	"icons": {
		"48": "icon48.PNG",
		"128": "icon128.PNG"
	},
	"content_scripts": [{
			"js": ["script.js"],
			"matches": ["*://www.pixiv.net/history.php"]
		}],
	"permissions": [
		"http://*.pixiv.net/*"
	]
}
